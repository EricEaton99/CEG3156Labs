	op_sel[0] = 1'b0;
	op_sel[0] = #320000 1'b1;
end 

top_level_vlg_sample_tst tb_sample (
	.A(A),
	.B(B),
	.op_sel(op_sel),
	.sampler_tx(sampler)
);

top_level_vlg_check_tst tb_out(
	.c_out(c_out),
	.overflow(overflow),
	.result(result),
	.zero(zero),
	.sampler_rx(sampler)
);
endmodule

