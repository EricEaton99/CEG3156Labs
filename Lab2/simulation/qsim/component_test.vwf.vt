module top_level_vlg_vec_tst();
// constants                                           
// general purpose registers
reg clk;
reg global_reset;
reg [2:0] output_sel;
// wires                                               
wire [7:0] output_sys;

wire sampler;                             

// assign statements (if any)                          
top_level i1 (
// port map - connection between master ports and signals/registers   
	.clk(clk),
	.global_reset(global_reset),
	.output_sel(output_sel),
	.output_sys(output_sys)
);

// global_reset
initial
begin
	global_reset = 1'b0;
	global_reset = #60000 1'b1;
end 

// clk
initial
begin
	repeat(8)
	begin
		clk = 1'b0;
		clk = #160000 1'b1;
		# 160000;
	end
	clk = 1'b0;
end 
// output_sel[ 2 ]
initial
begin
	repeat(8)
	begin
		output_sel[2] = 1'b0;
		output_sel[2] = #160000 1'b1;
		# 160000;
	end
	output_sel[2] = 1'b0;
end 
// output_sel[ 1 ]
initial
begin
	repeat(16)
	begin
		output_sel[1] = 1'b0;
		output_sel[1] = #80000 1'b1;
		# 80000;
	end
	output_sel[1] = 1'b0;
end 
// output_sel[ 0 ]
initial
begin
	repeat(32)
	begin
		output_sel[0] = 1'b0;
		output_sel[0] = #40000 1'b1;
		# 40000;
	end
	output_sel[0] = 1'b0;
end 

top_level_vlg_sample_tst tb_sample (
	.clk(clk),
	.global_reset(global_reset),
	.output_sel(output_sel),
	.sampler_tx(sampler)
);

top_level_vlg_check_tst tb_out(
	.output_sys(output_sys),
	.sampler_rx(sampler)
);
endmodule

