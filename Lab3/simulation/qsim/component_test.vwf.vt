module top_level_vlg_vec_tst();
// constants                                           
// general purpose registers
reg clk;
reg global_reset;
reg [2:0] output_sel;
// wires                                               
wire [31:0] IMEM_out;
wire [7:0] output_sys;

wire sampler;                             

// assign statements (if any)                          
top_level i1 (
// port map - connection between master ports and signals/registers   
	.clk(clk),
	.global_reset(global_reset),
	.IMEM_out(IMEM_out),
	.output_sel(output_sel),
	.output_sys(output_sys)
);

// clk
initial
begin
	repeat(12)
	begin
		clk = 1'b0;
		clk = #40000 1'b1;
		# 40000;
	end
	clk = 1'b0;
end 

// global_reset
initial
begin
	global_reset = 1'b0;
	global_reset = #50000 1'b1;
end 
// output_sel[ 2 ]
always
begin
	output_sel[2] = 1'b0;
	output_sel[2] = #20000 1'b1;
	#20000;
end 
// output_sel[ 1 ]
always
begin
	output_sel[1] = 1'b0;
	output_sel[1] = #10000 1'b1;
	#10000;
end 
// output_sel[ 0 ]
always
begin
	output_sel[0] = 1'b0;
	output_sel[0] = #5000 1'b1;
	#5000;
end 

top_level_vlg_sample_tst tb_sample (
	.clk(clk),
	.global_reset(global_reset),
	.output_sel(output_sel),
	.sampler_tx(sampler)
);

top_level_vlg_check_tst tb_out(
	.IMEM_out(IMEM_out),
	.output_sys(output_sys),
	.sampler_rx(sampler)
);
endmodule

